{
  "name": "@reputation-manager/source",
  "version": "0.0.0",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "nx run-many --target=serve --projects=web,api --parallel=2",
    "dev:all": "nx run-many --target=serve --projects=web,api,worker --parallel=3",
    "dev:api": "nx run api:serve:development --skip-nx-cache",
    "dev:web": "nx run web:dev",
    "build": "nx run-many --target=build --all --parallel=3",
    "test": "nx run-many --target=test --all --parallel=3",
    "lint": "nx run-many --target=lint --all --parallel=3",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "prisma:studio": "prisma studio --schema=libs/database/prisma/schema.prisma --config=libs/database/prisma/prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --schema=libs/database/prisma/schema.prisma --config=libs/database/prisma/prisma.config.ts",
    "prisma:deploy": "prisma migrate deploy --schema=libs/database/prisma/schema.prisma --config=libs/database/prisma/prisma.config.ts",
    "prisma:status": "prisma migrate status --schema=libs/database/prisma/schema.prisma --config=libs/database/prisma/prisma.config.ts",
    "prisma:generate": "prisma generate --schema=libs/database/prisma/schema.prisma",
    "prisma:seed": "node libs/database/prisma/seed.js",
    "prisma:reset": "prisma migrate reset --schema=libs/database/prisma/schema.prisma --config=libs/database/prisma/prisma.config.ts --force",
    "prisma:validate": "prisma validate --schema=libs/database/prisma/schema.prisma"
  },
  "private": true,
  "pnpm": {
    "overrides": {
      "hono": "^4.11.4"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
